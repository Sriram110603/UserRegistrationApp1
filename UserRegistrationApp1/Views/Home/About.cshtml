@{
    ViewData["Title"] = "About Us";
}
<div class="container my-5">
    <div class="text-center mb-4">
        <h1 class="display-4">@ViewData["Title"]</h1>
        <p class="lead">Learn more about our team and mission.</p>
    </div>

    <div class="row g-4">
        <div class="col-md-6">
            <h3>Our Mission</h3>
            <p>
                We aim to build secure, reliable, and user-friendly authentication systems that help developers get up and running quickly with login and registration features.
            </p>
        </div>
        <div class="col-md-6">
            <h3>Who We Are</h3>
            <p>
                We're a passionate team of developers focused on simplifying authentication flows while ensuring top-notch security standards.
            </p>
        </div>
    </div>

    <div class="text-center mt-5">
        <img src="data:image/jpeg;base64," class="img-fluid rounded shadow" alt="Team image" />
   <p>Our Team</p>
    </div>
</div>
